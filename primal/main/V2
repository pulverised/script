    Primal:SetTheme(Primal:GetCurrentTheme() == "Dark" and "Light" or "Dark")
    Primal:Notify({
        Title = "Theme Changed",
        Content = "Current theme: "..Primal:GetCurrentTheme(),
        Duration = 2
    })
end, 990)

